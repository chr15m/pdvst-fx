PARAMETERS = 3

# Name of first VST parameter
# used when CUSTOMGUI is false or VST host doesn't support custom editors
NAMEPARAMETER0 = pump
NAMEPARAMETER1 = rise-time
NAMEPARAMETER2 = fall-time

