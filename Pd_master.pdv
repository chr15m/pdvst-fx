# Unique four character VST plugin ID
ID = pdms

# Whether this is a VSTi
SYNTH = FALSE

# Whether there is a custom GUI (e.g. GrIPD)
CUSTOMGUI = FALSE

# Display Pd GUI or not
DEBUG = TRUE

# External libraries (comma delimited list, relative to pdvst/)
